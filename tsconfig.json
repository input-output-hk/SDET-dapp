{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "moduleResolution": "node",
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true,


    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "skipLibCheck": true,
    "paths": {
      "react": ["./node_modules/preact/compat/"],
      "react-dom": ["./node_modules/preact/compat/"],
      "rxjs": ["./node_modules/rxjs"]
    },
    "types": ["node"]
  }
}
